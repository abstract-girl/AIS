    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch User's service",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/user_service",
            "args": [""],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {"name": "DB_HOST", "value": "127.0.0.1"},
                {"name": "DB_PORT", "value": "6033"},
                {"name": "DB_LOGIN", "value": "stud"},
                {"name": "DB_PASSWORD", "value": "stud"},
                {"name": "DB_DATABASE", "value": "archdb"},
                {"name": "CACHE", "value": "cache:6379"}
            ],
            "externalConsole": false
        },

